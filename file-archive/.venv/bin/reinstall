#!/bin/bash

cd $VIRTUAL_ENV/..

MODULES=$(cat <<ML
file-archive-commons
file-archive-blob-storage-jpa
file-archive-blob-storage-s3
file-archive-metadata-storage-jpa
file-archive-metadata-storage-cassandra
file-archive-jaxrs
ML
)

echo $MODULES

for module in $MODULES; do
(
    cd $module
    mvn clean install
)
done
